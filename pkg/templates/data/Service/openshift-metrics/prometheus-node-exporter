apiVersion: v1
kind: Service
metadata:
  annotations:
    prometheus.io/scrape: "true"
  labels:
    app: prometheus-node-exporter
  name: prometheus-node-exporter
  namespace: openshift-metrics
spec:
  clusterIP: None
  ports:
  - name: scrape
    port: 9100
    protocol: TCP
    targetPort: 9100
  selector:
    app: prometheus-node-exporter
  sessionAffinity: None
  type: ClusterIP
