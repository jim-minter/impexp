apiVersion: rbac.authorization.k8s.io/v1
kind: ClusterRole
metadata:
  annotations:
    authorization.openshift.io/system-only: "true"
    rbac.authorization.kubernetes.io/autoupdate: "true"
  name: system:build-strategy-jenkinspipeline
rules:
- apiGroups:
  - ""
  - build.openshift.io
  resources:
  - builds/jenkinspipeline
  verbs:
  - create
